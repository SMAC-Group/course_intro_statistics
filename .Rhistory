geom_point(
aes(y = species_num - .15),
shape = "|",
size = 5,
alpha = .33
) +
ggdist::stat_halfeye(
aes(
y = species_num,
color = species,
fill = after_scale(colorspace::lighten(color, .5))
),
shape = 18, point_size = 3, interval_size = 1.8, adjust = .5,
.width = c(0, 1)
) +
theme_minimal() +
scale_color_manual(values = c("#3d6721", "#a86826", "#006c89"), guide = "none")
